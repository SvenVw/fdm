{
    "name": "@svenvw/fdm-docs",
    "version": "0.9.0",
    "private": true,
    "scripts": {
        "docusaurus": "docusaurus",
        "start": "docusaurus start",
        "watch-docs": "pnpm start",
        "build": "pnpm run build:api && docusaurus build",
        "build:api": "typedoc --options typedoc.json",
        "build-docs": "pnpm run build:api && docusaurus build",
        "lint": "markdownlint-cli2  '**/*.md' '#node_modules --format",
        "swizzle": "docusaurus swizzle",
        "deploy": "docusaurus deploy",
        "clear": "docusaurus clear",
        "serve": "docusaurus serve",
        "write-translations": "docusaurus write-translations",
        "write-heading-ids": "docusaurus write-heading-ids",
        "typecheck": "tsc",
        "preinstall": "npx only-allow pnpm"
    },
    "dependencies": {
        "@docusaurus/core": "3.9.2",
        "@docusaurus/faster": "3.9.2",
        "@docusaurus/preset-classic": "3.9.2",
        "@mdx-js/react": "^3.1.1",
        "clsx": "^2.1.1",
        "lucide-react": "^0.562.0",
        "prism-react-renderer": "^2.4.1",
        "react": "^19.2.4",
        "react-dom": "^19.2.4"
    },
    "devDependencies": {
        "@docusaurus/module-type-aliases": "3.9.2",
        "@docusaurus/tsconfig": "3.9.2",
        "@docusaurus/types": "3.9.2",
        "docusaurus-plugin-typedoc": "^1.4.2",
        "markdownlint-cli2": "^0.20.0",
        "typedoc": "catalog:",
        "typedoc-plugin-markdown": "^4.9.0",
        "typescript": "catalog:"
    },
    "browserslist": {
        "production": [
            ">0.5%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 3 chrome version",
            "last 3 firefox version",
            "last 5 safari version"
        ]
    },
    "engines": {
        "node": ">=24.0"
    },
    "packageManager": "pnpm@10.28.1"
}
